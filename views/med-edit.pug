doctype html
html
head
 title
 style(type="text/css").  
    body {
      font-style:italic ;
   }
link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous")
body
h1.text-center.my-4 Edit Page
div.container 
  div.row 
    div.col-md-6.offset-3
     form.form-group(action="/meds/edit/"+medList.id method="post")
       div.form-floating.my-3
          input.form-control#floatingName(type="text" name="medName" value=medList.medName)
          label(for="floatingName") Enter medicine name
       div.form-floating.my-3
         input.form-control#floatingCount(type="number" name="totalCount" value=medList.totalCount placeholder="")
         label(for="floatingCount") Enter count
       div.form-floating.my-3
          input.form-control#floatingBrand(type="text" name="brandName" value=medList.brandName placeholder="")
          label(for="floatingBrand") Enter brand
       button.btn.btn-primary.btn-lg(type="submit") Save